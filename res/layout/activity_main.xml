<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.brooks.testcellsignalstrength.MainActivity"
    tools:ignore="MergeRootFrame" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <TextView
            android:id="@+id/gsmLevelTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="GSM signal strength is null"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/cdmaLevelTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/gsmLevelTextView"
            android:text="CDMA signal strength is null"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/lteLevelTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/cdmaLevelTextView"
            android:text="LTE signal strength is null"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/wcdmaLevelTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/lteLevelTextView"
            android:text="WCDMA signal strength is null"
            android:textSize="20sp" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/lteLevelTextView"
            android:layout_alignTop="@id/lteLevelTextView"
            android:layout_centerHorizontal="false"
            android:layout_centerVertical="false"
            android:layout_toRightOf="@id/lteLevelTextView"
            android:src="@drawable/stat_sys_signal_null" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/cdmaLevelTextView"
            android:layout_alignTop="@+id/cdmaLevelTextView"
            android:layout_marginLeft="14dp"
            android:layout_toRightOf="@id/cdmaLevelTextView"
            android:src="@drawable/stat_sys_signal_null" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/gsmLevelTextView"
            android:layout_alignTop="@id/gsmLevelTextView"
            android:layout_toRightOf="@id/gsmLevelTextView"
            android:src="@drawable/stat_sys_signal_null" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/wcdmaLevelTextView"
            android:layout_alignTop="@id/wcdmaLevelTextView"
            android:layout_toRightOf="@id/wcdmaLevelTextView"
            android:src="@drawable/stat_sys_signal_null" />

    </RelativeLayout>

</FrameLayout>
